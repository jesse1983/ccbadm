{
    "extends": "airbnb",
    "installedESLint": true,
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "globals":{
      "window": true,
      "document": true,
      "fetch": true
    },
    "rules": {
      "react/prefer-stateless-function": 0,
      "react/prop-types": 0,
      "react/jsx-no-bind": 0,
      "arrow-body-style": 0,
      "class-methods-use-this": 0
    }
}
